generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  email       String @unique
  numberPhone String

  bio        String?
  profileUrl String?
  socials    String[]

  createAt DateTime @default(now())
}
